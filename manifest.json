{

    "name": "Text Speech",
    "version": "1.0",
    "manifest_version" : 1,
    "description": "A chrome extension that turns text into speech",

    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["Content.js"]
        }
        
    ],
    "browser_action": {
        "default_icon": "image.png",
        "default_popup": "index.hmtl"
    }
}